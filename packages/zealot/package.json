    "@types/events": "^3.0.0",
    "@types/node-fetch": "2",
    "abortcontroller-polyfill": "^1.7.3",
    "fkill": "7.2.1",
    "jest": "^27.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "prettier-eslint": "^8.8.2",
    "rollup": "^2.59.0",
    "ts-jest": "^27.1.3",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@js-joda/core": "^4.3.1",
    "cross-fetch": "^3.1.4",
    "event-source-polyfill": "^1.0.25",
    "events": "^3.3.0",
    "lodash": "^4.17.21",
    "node-fetch": "2"
  }
