  "name": "e2e-tests",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/e2e-tests",
  "implicitDependencies": ["zui"],
  "projectType": "library",
  "targets": {
    "e2e": {
      "executor": "@nxkit/playwright:test",
