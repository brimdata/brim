{
  "name": "@brimdata/zealot",
  "version": "0.0.17",
  "description": "The Javascript Client for Zed Lakes",
  "keywords": [
    "zed",
    "brim",
    "zjson",
    "zealot",
    "zedjs"
  ],
  "main": "dist/cjs/index.js",
  "module": "./dist/es/index.js",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/es/index.js"
  },
  "types": "./dist/cjs/index.d.ts",
  "license": "MIT",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brimdata/zealot"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "prepack": "yarn build && yarn build:es",
    "build": "tsc --outDir dist/cjs --declaration",
    "build:es": "rollup -c rollup.config.js",
    "watch": "yarn run build -- --watch",
    "test": "jest"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/events": "^3.0.0",
    "@types/node-fetch": "^2.5.12",
    "fkill": "7.2.1",
    "rollup": "^2.59.0",
    "ts-jest": "^27.1.2"
  },
  "dependencies": {
    "@js-joda/core": "^4.3.1",
    "cross-fetch": "^3.1.4",
    "events": "^3.3.0",
    "lodash": "^4.17.21"
  }
}
