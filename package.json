{
  "name": "brim-app",
  "description": "Brim Desktop App",
  "repository": "https://github.com/looky/looky-desktop",
  "license": "ISC",
  "version": "0.1.26",
  "main": "dist/js/electron/main.js",
  "author": "Brim Security, Inc.",
  "bin": {
    "release": "./scripts/release/index.js"
  },
  "scripts": {
    "start": "npm-run-all -s clean build:* -p electron watch:*",
    "test": "TZ=US/Pacific jest",
    "itest": "TZ=US/Pacific jest --config=itest/config.json --runInBand --verbose",
    "electron": "electron .",
    "build": "npm-run-all -p build:*",
    "watch": "npm-run-all -p watch:*",
    "build:js": "babel src/js -d dist/js --verbose",
    "build:css": "node-sass-chokidar src/css --output dist/css",
    "build:static": "cpx \"src/static/**\" dist/static",
    "watch:js": "npm run build:js -- --watch --skip-initial-build",
    "watch:css": "npm run build:css -- --watch --skip-initial",
    "watch:static": "npm run build:static -- --watch",
    "watch:reload": "livereload dist",
    "lint": "eslint .",
    "format": "prettier '{src,itest}/**/*.{js,scss}' --write --loglevel warn",
    "flow": "flow",
    "check": "npm-run-all -p format lint flow test",
    "clean": "node scripts/clean",
    "release": "node scripts/release --win32 --darwin",
    "prerelease": "npm run build",
    "prebuild": "npm run clean"
  },
  "jest": {
    "notify": false,
    "notifyMode": "change",
    "setupFilesAfterEnv": [
      "./src/js/test/setup.js"
    ],
    "testURL": "http://localhost",
    "roots": [
      "./src/js"
    ],
    "collectCoverageFrom": [
      "**/*.js",
      "!**/flow-typed/**",
      "!**/test/**"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "prettier": {
    "semi": false,
    "bracketSpacing": false,
    "arrowParens": "always"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "babel-plugin-inline-react-svg": "^1.1.0",
    "babel-plugin-source-map-support": "^2.0.1",
    "commander": "^2.19.0",
    "cpx": "^1.5.0",
    "electron": "^4.2.4",
    "electron-devtools-installer": "^2.2.4",
    "electron-installer-dmg": "^2.0.0",
    "electron-packager": "^13.1.1",
    "electron-winstaller": "^3.0.4",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.15.3",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-react": "^7.12.4",
    "flow-bin": "^0.95.1",
    "flow-typed": "^2.5.1",
    "import-js": "^3.2.0",
    "jest": "^24.5.0",
    "jest-junit": "^6.4.0",
    "livereload": "^0.8.0",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.16.4",
    "prettier-eslint": "^8.8.2",
    "spectron": "^6.0.0",
    "whatwg-fetch": "^3.0.0",
    "winston": "^3.2.1"
  },
  "dependencies": {
    "animejs": "^3.1.0",
    "base64-js": "^1.3.0",
    "classnames": "^2.2.6",
    "d3": "^5.9.2",
    "electron-dl": "^1.13.0",
    "electron-is-dev": "^1.0.1",
    "fs-extra": "^7.0.0",
    "lodash": "^4.17.14",
    "lookytalk": "git+https://github.com/looky-cloud/lookytalk.git#v0.1.13",
    "md5": "^2.2.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.21",
    "opn": "^6.0.0",
    "prismjs": "^1.16.0",
    "react": "^16.8.4",
    "react-day-picker": "^7.3.0",
    "react-dom": "^16.8.5",
    "react-measure": "^2.2.4",
    "react-redux": "^7.1.0-alpha.4",
    "react-transition-group": "^2.7.1",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.3",
    "reselect": "^4.0.0",
    "source-map-support": "^0.5.12",
    "valid-url": "^1.0.9"
  }
}
